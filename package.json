{
  "name": "hackathon",
  "version": "1.0.0",
  "description": "Página para el concurso Hackathon intercentros que se desarrolla cada año en Utrera ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compilar:sass": "sass --watch src/scss:build/css",
    "purgar": "purgecss --css build/css/app.css --content index.html --output build/css/app.min.css",
    "purgar2": "purgecss -css build/css/app.css -con index.html -o build/css/app.min.css",
    "optimizar": "cssnano build/css/app.min.css build/css/app.min.css",
    "build:css": "npm run purgar && npm run optimizar"
  },
  "author": "Yohanna Gelo",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "purgecss": "^7.0.2",
    "sass": "^1.85.1"
  }
}
